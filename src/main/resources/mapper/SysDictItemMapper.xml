<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ymm.microservices.usercenter.mapper.SysDictItemMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ymm.microservices.usercenter.entity.SysDictItem">
        <id column="id" property="id"/>
        <result column="dict_id" property="dictId"/>
        <result column="item_key" property="itemKey"/>
        <result column="item_value" property="itemValue"/>
        <result column="description" property="description"/>
        <result column="status" property="status"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, dict_id, item_key, item_value, description, status
    </sql>

</mapper>
